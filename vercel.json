{
  "outputDirectory": "dist/browser",
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|webp|svg|ico|json|txt|woff|woff2|ttf|eot))$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}